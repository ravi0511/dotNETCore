@model ViewModelDTO
@{
    ViewBag.Title = Model.PageTitle;
}
<h1>@Model.PageTitle</h1>
<table>
    <thead>
        <tr>
            <td>ID</td>
            <td>Name</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var empDetails in Model.empDetails)
        {
            <tr>
                <td>@empDetails.ID</td>
                <td>@empDetails.Name</td>
            </tr>
        }
    </tbody>
</table>