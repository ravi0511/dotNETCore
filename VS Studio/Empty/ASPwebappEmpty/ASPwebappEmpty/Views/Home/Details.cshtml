@model Employee
@{
    ViewBag.Title = ViewBag.Title;
}
<h1>@ViewBag.Title</h1>
<table>
    <thead>
        <tr>
            <td>ID</td>
            <td>Name</td>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.ID</td>
            <td>@Model.Name</td>
        </tr>
    </tbody>
</table>